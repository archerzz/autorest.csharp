{
  "name": "adp-sample",
  "version": "2022-11-30-preview",
  "description": "ADP Data Management API",
  "main": "index.js",
  "config": {
    "OutputFileNameOpenApi2": "data-management-openapi-v2.json",
    "OutputFileNameOpenApi3": "data-management-openapi-v3.json",
    "ClientPath": "../../../clients/Clients.DataManagement.vDraftCompliantV2",
    "ClientNamespace": "Microsoft.Azure.Adp.DataManagement.Clients.vDraftCompliantV2",
    "PublishOutputFileName": "../../draftcompliant/apispec.core.datamanagement.2022-11-30-draftcompliant.json",
    "PathToAzureApiReviewRepository": "C:/Users/igmakhli/source/repos/imakhlin/azure-rest-api-specs-pr/specification/adp/data-plane/data-management/preview",
    "AutorestManifestFile": "../data-plane/common/build/autorest.md"
  },
  "scripts": {
    "buildPublicOpenApi2": "pwsh ./common/build/compile.ps1 -IsPublicApi -InputCadl . -OutputPath ./generated/cadl-autorest/public -TargetFileName %npm_package_config_OutputFileNameOpenApi2%",
    "buildInternalOpenApi2": "pwsh ./common/build/compile.ps1 -InputCadl ./internal-main.cadl -OutputPath ./generated/cadl-autorest/internal -TargetFileName %npm_package_config_OutputFileNameOpenApi2%",
    "buildPublicOpenApi3": "pwsh ./common/build/compile.ps1 -IsPublicApi -InputCadl . -OutputPath ./generated/openapi3/public -Emitter @cadl-lang/openapi3 -TargetFileName %npm_package_config_OutputFileNameOpenApi3%",
    "buildInternalOpenApi3": "pwsh ./common/build/compile.ps1 -InputCadl ./internal-main.cadl -OutputPath ./generated/openapi3/internal -Emitter @cadl-lang/openapi3 -TargetFileName %npm_package_config_OutputFileNameOpenApi3%",
    "publish": "pwsh ./common/build/publish.ps1 -SourcePath ./generated/cadl-autorest/public/%npm_package_config_OutputFileNameOpenApi2% -OutputPath %npm_package_config_PublishOutputFileName% -PathToAzureApiReviewRepository %npm_package_config_PathToAzureApiReviewRepository%/%npm_package_version%",
    "generateAutorestClient": "pwsh ./common/build/autorest-generator.ps1 -InputOpenApiSpecFile ./generated/cadl-autorest/public/%npm_package_config_OutputFileNameOpenApi2% -OutputPath %npm_package_config_ClientPath%/generated -Namespace %npm_package_config_ClientNamespace% -AutorestManifestFile %npm_package_config_AutorestManifestFile%",
    "generateOpenApi3Client": "pwsh ./common/build/autorest-generator.ps1 -InputOpenApiSpecFile ./generated/openapi3/public/%npm_package_config_OutputFileNameOpenApi2% -OutputPath %npm_package_config_ClientPath%/generated -Namespace %npm_package_config_ClientNamespace% -AutorestManifestFile %npm_package_config_AutorestManifestFile%",
    "validateSpec": "pwsh ./common/build/validate-spec.ps1 -OpenApiSpecFile ./generated/cadl-autorest/public/%npm_package_config_OutputFileNameOpenApi2% && pwsh ./common/build/validate-examples.ps1 -OpenApiSpecFile ./generated/cadl-autorest/public/%npm_package_config_OutputFileNameOpenApi2%",
    "format": "npx cadl format --pretty ./**/*.cadl"
  },
  "keywords": [],
  "author": "igmakhli@microsoft.com",
  "license": "MIT",
  "dependencies": {
    "@azure-tools/cadl-autorest": "latest",
    "@azure-tools/cadl-azure-core": "latest",
    "@cadl-lang/openapi": "latest",
    "@cadl-lang/openapi3": "latest",
    "@cadl-lang/rest": "latest",
    "@cadl-lang/versioning": "latest"
  },
  "devDependencies": {
    "@cadl-lang/compiler": "latest",
    "prettier": "^2.7.1"
  }
}
